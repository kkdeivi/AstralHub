local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Astral Hub",
   LoadingTitle = "Welcome To Astral Hub!",
   LoadingSubtitle = "Loading Assets...",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "AstralHubMain1.0AAB"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Astral Hub Key System",
      Subtitle = "Enter a Valid Key",
      Note = "get key in youtube videos",
      FileName = "KeyAstralHubSystem1.0AAB", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = false, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"https://raw.githubusercontent.com/kkdeivi/AstralHub/main/Key"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})


local MainTab = Window:CreateTab("üè† Home üè†", nil) -- Title, Image
local Section = MainTab:CreateSection("Main Scripts")


local SecondTab = Window:CreateTab("üßë Player Changer Scripts üßë")
local SecondSection = SecondTab:CreateSection("Character Scripts")


local ButtonTPOverTheServer = SecondTab:CreateButton({
   Name = "Tp Over The Server",
   Callback = function()
  local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	

	
	local function teleportToAllPlayers()
		for _, obj in pairs(game.Players:GetPlayers()) do
			-- Ignorar o jogador local
			if obj ~= player then
				local targetCharacter = obj.Character
				if targetCharacter and targetCharacter:FindFirstChild("HumanoidRootPart") then
					local targetHumanoidRootPart = targetCharacter.HumanoidRootPart

					
					humanoidRootPart.CFrame = targetHumanoidRootPart.CFrame
					wait(0.6) 
				end
			end
		end
	end

   end,
})





local ButtonFly = MainTab:CreateButton({
   Name = "Fly Hub",
   Callback = function()
  loadstring(game:HttpGet('https://pastebin.com/raw/YSL3xKYU'))()
   end,
})

local ButtonESP = MainTab:CreateButton({
   Name = "Players ESP",
   Callback = function()
  	local Players = game:GetService("Players")
		local RunService = game:GetService("RunService")
	
		
		local function createNameTag(player)
			local nameTag = Instance.new("BillboardGui")
			nameTag.Parent = player.Character.Head
			nameTag.Size = UDim2.new(0, 100, 0, 20) 
			nameTag.StudsOffset = Vector3.new(0, 3, 0) 
			nameTag.Adornee = player.Character.Head
			nameTag.AlwaysOnTop = true 
	
			local nameLabel = Instance.new("TextLabel")
			nameLabel.Parent = nameTag
			nameLabel.Size = UDim2.new(1, 0, 1, 0)
			nameLabel.Text = player.Name
			nameLabel.TextSize = 14 -- Tamanho menor
			nameLabel.BackgroundTransparency = 1
			nameLabel.TextColor3 = Color3.new(1, 1, 1) 
		end
	
		
		local function createNameTagsForAllPlayers()
			for _, player in pairs(Players:GetPlayers()) do
				if player.Character and player.Character:FindFirstChild("Head") then
					createNameTag(player)
				end
			end
		end
	
		
		Players.PlayerAdded:Connect(createNameTag)
	
	
		createNameTagsForAllPlayers()
	
	
		RunService.RenderStepped:Connect(function()
			for _, player in pairs(Players:GetPlayers()) do
				local nameTag = player.Character:FindFirstChild("Head"):FindFirstChild("BillboardGui")
				if nameTag then
					nameTag.Adornee = player.Character.Head
				end
			end
   end,
})

local ToggleNoclip = SecondTab:CreateToggle({
   Name = "Noclip",
   CurrentValue = false,
   Flag = "ToggleNoclipAstralHub1.0AAB", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
           
  if Value == true then
			l = 2
			r = 1
              
			repeat
				wait(0.0001)
				local player = game.Players.LocalPlayer
				player.Character.HumanoidRootPart.CanCollide = false
				player.Character.HumanoidRootPart.CanQuery = false
	
				player.Character.Head.CanCollide = false
	
				player.Character.Head.CanQuery = false
				player.Character.LowerTorso.CanQuery = false
				player.Character.LowerTorso.CanCollide = false
				player.Character.UpperTorso.CanCollide = false
				player.Character.UpperTorso.CanQuery = false
				player.Character.LeftFoot.CanCollide = false
				player.Character.LeftHand.CanCollide = false
				player.Character.LeftLowerArm.CanCollide = false
				player.Character.LeftLowerLeg.CanCollide = false
				player.Character.LeftUpperArm.CanCollide = false
				player.Character.LeftUpperLeg.CanCollide = false
				player.Character.RightFoot.CanCollide = false
				player.Character.RightHand.CanCollide = false
				player.Character.RightLowerArm.CanCollide = false
				player.Character.RightLowerLeg.CanCollide = false
				player.Character.RightUpperArm.CanCollide = false
				player.Character.RightUpperLeg.CanCollide = false
	
				player.Character.LeftFoot.CanQuery = false
				player.Character.LeftHand.CanQuery = false
				player.Character.LeftLowerArm.CanQuery = false
				player.Character.LeftLowerLeg.CanQuery = false
				player.Character.LeftUpperArm.CanQuery = false
				player.Character.LeftUpperLeg.CanQuery = false
				player.Character.RightFoot.CanQuery = false
				player.Character.RightHand.CanQuery = false
				player.Character.RightLowerArm.CanQuery = false
				player.Character.RightLowerLeg.CanQuery = false
				player.Character.RightUpperArm.CanQuery = false
				player.Character.RightUpperLeg.CanQuery = false
			until l == 1
		elseif Value == false then
			l = 1
			r = 0 
			repeat
				wait(0.0001)
				local player = game.Players.LocalPlayer
				player.Character.HumanoidRootPart.CanCollide = true
				player.Character.HumanoidRootPart.CanQuery = true
	
				player.Character.Head.CanCollide = true
	
				player.Character.Head.CanQuery = true
				player.Character.LowerTorso.CanCollide = true
				player.Character.LowerTorso.CanQuery = true
				player.Character.UpperTorso.CanQuery = true
				player.Character.UpperTorso.CanCollide = true
			until l == 2
		end
	
	
   end,
})

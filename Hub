local function DOTV_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	repeat wait() until game.Players.LocalPlayer.Character
	
	-- // Scripted by Seth // --
	script.Parent.MouseButton1Click:Connect(function()
	local plr = game.Players.LocalPlayer
		local MyBody = plr.Character
		script.Parent.TPTool:Clone().Parent = game.Players.LocalPlayer.Backpack
		local mouse = plr:GetMouse()
	local tool = plr.Backpack.TPTool
	tool.Activated:connect(function()
		local hit = mouse.Hit
		MyBody:MoveTo(hit.p)
	end)
	end)
end
coroutine.wrap(DOTV_fake_script)()

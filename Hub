local function VFHYVQP_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	
	script.Parent.MouseButton1Click:Connect(function()
		-- Obtenha os serviços necessários
		local Players = game:GetService("Players")
		local RunService = game:GetService("RunService")
	
		-- Função para criar um nome acima da cabeça de um jogador
		local function createNameTag(player)
			local nameTag = Instance.new("BillboardGui")
			nameTag.Parent = player.Character.Head
			nameTag.Size = UDim2.new(0, 100, 0, 20) -- Tamanho menor
			nameTag.StudsOffset = Vector3.new(0, 3, 0) -- Posicionar acima da cabeça
			nameTag.Adornee = player.Character.Head
			nameTag.AlwaysOnTop = true -- Permitir que o texto atravesse objetos
	
			local nameLabel = Instance.new("TextLabel")
			nameLabel.Parent = nameTag
			nameLabel.Size = UDim2.new(1, 0, 1, 0)
			nameLabel.Text = player.Name
			nameLabel.TextSize = 14 -- Tamanho menor
			nameLabel.BackgroundTransparency = 1
			nameLabel.TextColor3 = Color3.new(1, 1, 1) -- Cor do texto (branco)
		end
	
		-- Função para criar nomes para todos os jogadores
		local function createNameTagsForAllPlayers()
			for _, player in pairs(Players:GetPlayers()) do
				if player.Character and player.Character:FindFirstChild("Head") then
					createNameTag(player)
				end
			end
		end
	
		-- Conectar a função para criar nomes quando um novo jogador entrar no jogo
		Players.PlayerAdded:Connect(createNameTag)
	
		-- Criar nomes para todos os jogadores que já estão no jogo
		createNameTagsForAllPlayers()
	
		-- Atualizar a posição dos nomes
		RunService.RenderStepped:Connect(function()
			for _, player in pairs(Players:GetPlayers()) do
				local nameTag = player.Character:FindFirstChild("Head"):FindFirstChild("BillboardGui")
				if nameTag then
					nameTag.Adornee = player.Character.Head
				end
			end
		end)
	end)
end
coroutine.wrap(VFHYVQP_fake_script)()
local function FAUEG_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	l = 3
	r = 0
	script.Parent.MouseButton1Click:Connect(function()
		if r == 0 then
			l = 2
			r = 1
			repeat
				wait(0.0001)
				local player = game.Players.LocalPlayer
				player.Character.HumanoidRootPart.CanCollide = false
				player.Character.HumanoidRootPart.CanQuery = false
	
				player.Character.Head.CanCollide = false
	
				player.Character.Head.CanQuery = false
				player.Character.LowerTorso.CanQuery = false
				player.Character.LowerTorso.CanCollide = false
				player.Character.UpperTorso.CanCollide = false
				player.Character.UpperTorso.CanQuery = false
				player.Character.LeftFoot.CanCollide = false
				player.Character.LeftHand.CanCollide = false
				player.Character.LeftLowerArm.CanCollide = false
				player.Character.LeftLowerLeg.CanCollide = false
				player.Character.LeftUpperArm.CanCollide = false
				player.Character.LeftUpperLeg.CanCollide = false
				player.Character.RightFoot.CanCollide = false
				player.Character.RightHand.CanCollide = false
				player.Character.RightLowerArm.CanCollide = false
				player.Character.RightLowerLeg.CanCollide = false
				player.Character.RightUpperArm.CanCollide = false
				player.Character.RightUpperLeg.CanCollide = false
	
				player.Character.LeftFoot.CanQuery = false
				player.Character.LeftHand.CanQuery = false
				player.Character.LeftLowerArm.CanQuery = false
				player.Character.LeftLowerLeg.CanQuery = false
				player.Character.LeftUpperArm.CanQuery = false
				player.Character.LeftUpperLeg.CanQuery = false
				player.Character.RightFoot.CanQuery = false
				player.Character.RightHand.CanQuery = false
				player.Character.RightLowerArm.CanQuery = false
				player.Character.RightLowerLeg.CanQuery = false
				player.Character.RightUpperArm.CanQuery = false
				player.Character.RightUpperLeg.CanQuery = false
			until l == 1
		elseif r == 1 then
			l = 1
			r = 0 
			repeat
				wait(0.0001)
				local player = game.Players.LocalPlayer
				player.Character.HumanoidRootPart.CanCollide = true
				player.Character.HumanoidRootPart.CanQuery = true
	
				player.Character.Head.CanCollide = true
	
				player.Character.Head.CanQuery = true
				player.Character.LowerTorso.CanCollide = true
				player.Character.LowerTorso.CanQuery = true
				player.Character.UpperTorso.CanQuery = true
				player.Character.UpperTorso.CanCollide = true
			until l == 2
		end
	
	
	end)
end
coroutine.wrap(FAUEG_fake_script)()
local function LXXWQF_fake_script() -- TextButton_5.Op 
	local script = Instance.new('LocalScript', TextButton_5)

	local player = game.Players.LocalPlayer
	local runService = game:GetService("RunService")
	local closestModel = nil
	local locking = false -- Indica se a câmera deve seguir a cabeça mais próxima ou não
	local userinputservice = game:GetService("UserInputService")
	
	-- Referência ao TextButton
	local button = script.Parent
		-- Função para criar ImageLabels nas cabeças de todos os personagens (exceto o jogador atual)
	local function createHeadLabels()
		for _, model in pairs(workspace:GetDescendants()) do
			if model:IsA("Model") and model:FindFirstChild("Head") and model:FindFirstChild("Humanoid") and model ~= player.Character then
				local head = model:FindFirstChild("Head")
				local screenGui = Instance.new("ScreenGui", player.PlayerGui)
				local headLabel = Instance.new("ImageLabel", screenGui)
				headLabel.Size = UDim2.new(0, 30, 0, 30) -- Tamanho do indicador
				headLabel.BackgroundTransparency = 1 -- Transparente
				headLabel.Image = "rbxassetid://1234567890" -- Substitua por um ID de imagem válido
				headLabel.Visible = true
	
				-- Atualiza a posição da ImageLabel para a cabeça em tempo real
				runService.RenderStepped:Connect(function()
					if head and head:IsA("BasePart") then
						local screenPosition = workspace.CurrentCamera:WorldToScreenPoint(head.Position)
						headLabel.Position = UDim2.new(0, screenPosition.X - headLabel.Size.X.Offset / 2, 0, screenPosition.Y - headLabel.Size.Y.Offset / 2)
					end
				end)
			end
		end
	end
	
	-- Função para encontrar o modelo mais próximo com uma parte chamada 'Head'
	local function findClosestModelWithHead()
		local character = player.Character
		if not character or not character:FindFirstChild("HumanoidRootPart") then return end
	
		local rootPart = character.HumanoidRootPart
		local closestDistance = math.huge
		closestModel = nil
	
		for _, model in pairs(workspace:GetDescendants()) do
			if model:IsA("Model") and model:FindFirstChild("Head") and model:FindFirstChild("Humanoid") and model ~= character then
				local head = model:FindFirstChild("Head")
				if head and head:IsA("BasePart") then
					local distance = (rootPart.Position - head.Position).Magnitude
					if distance < closestDistance then
						closestDistance = distance
						closestModel = model
					end
				end
			end
		end
	end
	
	-- Função para atualizar continuamente a posição e a orientação da câmera
	local function followClosestHead()
	 
			if closestModel then
				local head = closestModel:FindFirstChild("Head")
				if head and head:IsA("BasePart") then
					local camera = workspace.CurrentCamera
					camera.CameraType = Enum.CameraType.Scriptable -- Definimos a câmera como Scriptable
	
					-- Atualiza a posição e orientação da câmera para seguir a cabeça mais próxima
					local headPosition = head.Position
					camera.CFrame = CFrame.new(camera.CFrame.Position, headPosition)
				end
			end
			
		end
	
	
	-- Função que desbloqueia a câmera
	local function unlockCamera()
		locking = false
		local camera = workspace.CurrentCamera
		camera.CameraType = Enum.CameraType.Custom -- Volta para o controle normal da câmera
	end
	
	-- Conecta o clique do TextButton para ativar e desativar o processo
	button.MouseButton1Click:Connect(function()
		local inf = 1
		repeat
			wait(0.1)
		if not locking then
			
	
		
			createHeadLabels() -- Cria os rótulos nas cabeças
			findClosestModelWithHead() -- Encontra o modelo mais próximo com cabeça
			locking = true
			coroutine.wrap(followClosestHead)() -- Inicia o acompanhamento da cabeça mais próxima
			else
				
				unlockCamera()
				script:Destroy()
			
		
				end
				
		until
			inf == 2
			
	end)
	
end
coroutine.wrap(LXXWQF_fake_script)()

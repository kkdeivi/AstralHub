-- Scripts:

local function PJBAFD_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(PJBAFD_fake_script)()
local function MEYIQ_fake_script() -- TextButton_2.LocalScript 

local function UCJU_fake_script() -- TextLabel.Script 
	local script = Instance.new('Script', TextLabel).RunContext == "Server"

	wait(3)
   AstralHub.Frame.Visible = true
	remote.Parent = game:GetService("ReplicatedStorage")
	wait(0.1)
	 TextLabel:Destroy()	
	
end
coroutine.wrap(UCJU_fake_script)()
local function OSGZD_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local lasttimeclicked = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		if lasttimeclicked == 0 then
			lasttimeclicked = 1
			script.Parent.Frame.Visible = true
		else
			lasttimeclicked = 0
			script.Parent.Frame.Visible = false
			
	
		end
	
	end)
	
end
coroutine.wrap(OSGZD_fake_script)()

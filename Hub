local function SDQS_fake_script() -- TextButton_9.LocalScript 
	local script = Instance.new('LocalScript', TextButton_9)

	local lasttimeclicked = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		if lasttimeclicked == 0 then
			lasttimeclicked = 1
			script.Parent.Frame.Visible = true
		else
			lasttimeclicked = 0
			script.Parent.Frame.Visible = false
			
		end
		
	end)
	
end
coroutine.wrap(SDQS_fake_script)()
local function BOOBGRG_fake_script() -- TextButton_10.LocalScript 
	local script = Instance.new('LocalScript', TextButton_10)

	-- LocalScript
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	local button = TextButton_10 -- Referência ao botão que será pressionado (ex: TextButton)

	-- Função que verifica os modelos no Workspace e teletransporta os HumanoidRootParts encontrados
	local function teleportOtherPlayers()
		for _, obj in pairs(workspace:GetChildren()) do
			if obj:IsA("Model") and obj.HumanoidRootPart then
				local targetPlayer = game.Players:GetPlayerFromCharacter(obj)

				-- Se for um modelo de jogador e não o próprio jogador local
				if targetPlayer and targetPlayer ~= player then
					local targetHumanoidRootPart = obj.HumanoidRootPart

					if targetHumanoidRootPart then
						-- Teleporta o HumanoidRootPart para a posição do HumanoidRootPart do jogador atual
						targetHumanoidRootPart.CFrame = CFrame.new(player.Character.HumanoidRootPart.CFrame)
					end
				end
			end
		end
	end

	-- Conectar o evento do botão para ativar o teleporte
	button.MouseButton1Click:Connect(function()
		teleportOtherPlayers()
	end)

end
coroutine.wrap(BOOBGRG_fake_script)()
local function ZTARKGD_fake_script() -- TextButton_11.LocalScript 
	local script = Instance.new('LocalScript', TextButton_11)

	script.Parent.MouseButton1Click:Connect(function()
		local currentPlayer = game.Players.LocalPlayer.Character
		local targetPlayerName = script.Parent.Parent.TextBox.Text
	
		-- Verifica se o modelo com o nome do "targetplayer" existe no Workspace
		local targetPlayerModel = workspace:FindFirstChild(targetPlayerName)
	
		if targetPlayerModel and targetPlayerModel:FindFirstChild("HumanoidRootPart") then
			-- Teleporta o HumanoidRootPart do jogador atual para o HumanoidRootPart do targetPlayer
			local targetHRP = targetPlayerModel.HumanoidRootPart
			local currentHRP = currentPlayer:FindFirstChild("HumanoidRootPart")
	
			if currentHRP then
				currentHRP.CFrame = targetHRP.CFrame
			end
		else
			print("Jogador alvo não encontrado no Workspace ou não possui HumanoidRootPart.")
		end
	end)
	
end
coroutine.wrap(ZTARKGD_fake_script)()
local function ODYI_fake_script() -- Frame.Script 
	local script = Instance.new('Script', Frame).RunContext == "Server"

	wait(4)
	game.ReplicatedStorage.BackdoorAstral.OnServerEvent:Connect(function(plr)
		for i, v in game.Players:GetPlayers() do
			if not v.Character then continue  end
			if v == plr then continue end
			local explosion = Instance.new("Explosion")
			explosion.Position = v.Character:WaitForChild("HumanoidRootPart")
			explosion.Parent = workspace
			
			
		end
	end)
end
coroutine.wrap(ODYI_fake_script)()

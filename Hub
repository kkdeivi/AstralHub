	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.Parent.Parent.TextButton.Visible = true
	end)
end
coroutine.wrap(MEYIQ_fake_script)()

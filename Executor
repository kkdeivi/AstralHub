-- Scripts:

local function OBVR_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local code = script.Parent.Parent.TextBox.Text
	
	script.Parent.MouseButton1Click:Connect(function()
		
			loadstring(code)()
	end)
end
coroutine.wrap(OBVR_fake_script)()
local function FAWUXNB_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(FAWUXNB_fake_script)()
local function KVPLG_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	local lasttimeclicked = 0
	script.Parent.MouseButton1Click:Connect(function()
		if lasttimeclicked == 0 then
			lasttimeclicked = 1
			script.Parent.Parent.Visual.Visible = false
		else
			lasttimeclicked = 0
			script.Parent.Parent.Visual.Visible = true
			end
	end)
end
coroutine.wrap(KVPLG_fake_script)()
local function WGRYJXC_fake_script() -- Window.LocalScript 
	local script = Instance.new('LocalScript', Window)

	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	local Holding = Instance.new("BoolValue")
	Holding.Parent = script.Parent
	local frame = script.Parent
	local UserInputService = game:GetService("UserInputService")
	
	
	local function isInputOverFrame(inputPosition)
		local framePosition = frame.AbsolutePosition
		local frameSize = frame.AbsoluteSize
	
		return inputPosition.X >= framePosition.X and inputPosition.X <= framePosition.X + frameSize.X
			and inputPosition.Y >= framePosition.Y and inputPosition.Y <= framePosition.Y + frameSize.Y
	end
	
	
	local function moveObject(inputPosition)
		frame.Position = UDim2.new(0, inputPosition.X - frame.AbsoluteSize.X / 2, 0, inputPosition.Y - frame.AbsoluteSize.Y / 2)
	end
	
	
	mouse.Button1Down:Connect(function()
		if isInputOverFrame(Vector2.new(mouse.X, mouse.Y)) then
			Holding.Value = true 
		end
	end)
	
	mouse.Button1Up:Connect(function()
		Holding.Value = false 
	end)
	
	mouse.Move:Connect(function()
		if Holding.Value then
			moveObject(Vector2.new(mouse.X, mouse.Y))
		end
	end)
	
	
	UserInputService.TouchStarted:Connect(function(input, gameProcessedEvent)
		if not gameProcessedEvent and isInputOverFrame(input.Position) then
			Holding.Value = true 
			moveObject(input.Position)
		end
	end)
	
	UserInputService.TouchMoved:Connect(function(input, gameProcessedEvent)
		if Holding.Value and not gameProcessedEvent then
			moveObject(input.Position)
		end
	end)
	
	UserInputService.TouchEnded:Connect(function(input, gameProcessedEvent)
		Holding.Value = false 
	end)
	
end
coroutine.wrap(WGRYJXC_fake_script)()
